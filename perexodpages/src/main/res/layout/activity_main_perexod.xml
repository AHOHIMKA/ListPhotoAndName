<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/_5365"
    tools:context=".MainActivityPerexod">

    <Button
        android:id="@+id/button3"
        android:layout_width="173dp"
        android:layout_height="49dp"
        android:layout_marginTop="140dp"
        android:onClick="Nazad"
        android:text="@string/Button1_Back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/HumanName" />

    <Button
        android:id="@+id/button"
        android:layout_width="173dp"
        android:layout_height="49dp"
        android:layout_marginTop="84dp"
        android:onClick="Dalee1"
        android:text="@string/Button1_Next"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/HumanName" />

    <EditText
        android:id="@+id/HumanName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="452dp"
        android:ems="10"
        android:hint="@string/TextBox_name"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>